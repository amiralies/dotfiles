#!/bin/sh

xrandr --newmode "1360x768_60.00"  84.72  1360 1424 1568 1776  768 769 772 795  -HSync +Vsync
xrandr --addmode eDP1 "1360x768_60.00"
xrandr --output HDMI1 --auto --output eDP1 --mode "1360x768_60.00" --right-of HDMI1 --output DP1 --auto --left-of HDMI1
feh --bg-scale ~/Pictures/Wallpapers/current.jpg --bg-scale ~/Pictures/Wallpapers/current.jpg

